{
  "name": "financial",
  "version": "2.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --color --inline --hot",
    "build": "cross-env NODE_ENV=production webpack -p --progress --hide-modules",
    "test": "jest --no-cache"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      [
        "inferno",
        {
          "imports": true
        }
      ],
      "transform-flow-strip-types"
    ]
  },
  "jest": {
    "bail": true,
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "./config/babelTransform.js"
    }
  },
  "dependencies": {
    "bulma": "^0.3.1",
    "inferno": "^1.2.2",
    "inferno-component": "^1.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-inferno": "^1.7.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-react-jsx": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-runtime": "^6.22.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.2",
    "file-loader": "^0.10.0",
    "flow-bin": "^0.38.0",
    "inferno-devtools": "^1.2.2",
    "jest": "^18.1.0",
    "json-server": "^0.9.4",
    "node-sass": "^4.5.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2.2.0"
  }
}
